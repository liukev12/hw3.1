<h1><%= @place["name"] %></h1>

<% for entry in @entries %>
 
  <h2><%= entry["title"] %></h2>
  <ul>
  <li><%= entry["posted_on"] %></li>
  <li><%= entry["description"] %></li>
  </ul>

<% end %>

<p>
<a href="/entries/new?place_id=<%= @place["id"] %>">New Post</a>
</p>

<p>
<a href="/places">Return To All Places</a>
</p>